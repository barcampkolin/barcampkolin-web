{
	"name": "redbit/barcampkolin.cz",
	"description": "Web Kolínského barcampu",
	"license": [
		"MIT",
		"BSD-3-Clause",
		"GPL-2.0",
		"GPL-3.0"
	],
	"type": "project",
	"authors": [
		{
			"name": "Redbit s.r.o.",
			"homepage": "https://www.redbit.cz/"
		},
		{
			"name": "Jakub Bouček",
			"homepage": "https://www.jakub-boucek.cz/"
		},
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"homepage": "https://www.barcampkolin.cz/",
	"require": {
		"php": ">=8.1",
		"facebook/graph-sdk": "^5.7",
		"kdyby/forms-replicator": "^2.0",
		"latte/latte": "^2.5",
		"nette/application": "^3.0.6",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/database": "^3.0",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/mail": "^3.1",
		"nette/robot-loader": "^3.2",
		"nette/safe-stream": "^2.4",
		"nette/security": "^3.0",
		"nette/utils": "^3.0",
		"nextras/dbal": "^4.0",
		"nextras/orm": "^4.0",
		"nextras/secured-links": "^1.4",
		"redbitcz/debug-mode-enabler": "^5.0",
		"tracy/tracy": "^2.6",
		"ublaboo/datagrid": "^6.2"
	},
	"repositories": {
		"facebook/graph-sdk": {
			"type": "package",
			"package": {
				"name": "facebook/graph-sdk",
				"version": "5.7.0",
				"require": {
					"php": "*"
				},
				"source": {
					"type": "git",
					"url": "https://github.com/facebookarchive/php-graph-sdk.git",
					"reference": "5.7.0"
				}
			}
		}
	},
	"autoload": {
		"classmap": ["app/Model/exceptions.php"],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true
	}
}
